<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<display-name>servletDemo</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>myFirstServlet</servlet-name>
		<servlet-class>com.ibm.demo.MyFirstServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>DownloadServlet_ibm</display-name>
		<servlet-name>DownloadServlet_ibm</servlet-name>
		<servlet-class>com.ibm.demo.DownloadServlet_ibm</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>GetRequestHeader</display-name>
		<servlet-name>GetRequestHeader</servlet-name>
		<servlet-class>com.ibm.demo.GetRequestHeader</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>FormServlet</display-name>
		<servlet-name>FormServlet</servlet-name>
		<servlet-class>com.ibm.demo.FormServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myFirstServlet</servlet-name>
		<url-pattern>/myFirstServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DownloadServlet_ibm</servlet-name>
		<url-pattern>
		/DownloadServlet_ibm</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetRequestHeader</servlet-name>
		<url-pattern>
		/GetRequestHeader</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FormServlet</servlet-name>
		<url-pattern>
		/FormServlet</url-pattern>
	</servlet-mapping>
</web-app>